	<% if user_signed_in? %>
	<div class="subscribebar">
		<% if @subscriptions.blank? %>
		<table>

				&nbsp

			</table>
		<% else %>
	<table>
	  <tbody>
	    <% @subscriptions.each do |subscription| %>
	      <td>
<%= link_to subscription.title, watchsub_stream_path(subscription.user_id, title: subscription.title), "data-no-turbolink" => true %> 
		</td>
	    <% end %>
	  </tbody>
	</table>
	</div>
	<% end %>
	<% else %>
	<div class="subscribebar">
	<table>
	  <tbody>
	      <td>
			 &nbsp
		</td>
	  </tbody>
	</table>
	</div>

	<% end %>